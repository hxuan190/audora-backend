{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCloudFrontRead",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::processed-tracks/*"
      ],
      "Condition": {
        "StringEquals": {
          "s3:signatureAge": "86400"
        }
      }
    },
    {
      "Sid": "AllowBucketListing",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::audora"
      ]
    }
  ]
}